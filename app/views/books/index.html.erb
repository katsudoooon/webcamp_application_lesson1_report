<h2>New book</h2>
<%= form_with model: @book, local:true do |f| %>

  <label>Title</label>
  <%= f.text_field :title %>

  <label>Body</label>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>